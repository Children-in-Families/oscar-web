%div{ class: "form-group #{field_with(field_props['label'],errors)}" }
  %label.control-label{ class: required?(field_props['required'] || false) }
    %abbr{ title: 'required' }= '*' if field_props['required'] || false
    = field_props['label']
  = f.input field_props['label'], collection: field_props['values'].map { |f| [f['label'], f['label']] }, required: (field_props['required'] || false), prompt: field_props['placeholder'], input_html: { class: field_props['ClassName'], multiple: field_props['multiple'], name: "#{f.object_name}[#{field_props['label']}]" }, label: false
  = error_message(controller_name, field_message(field_props['label'], errors))
  -# %span.help-block
  -#   = field_message(field_props['label'], errors)
