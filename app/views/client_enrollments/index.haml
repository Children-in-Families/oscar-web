.ibox
  .ibox-title
    %h5= t('.program_lists')
    .ibox-tools
      = link_to 'Back', client_path(@client), class: 'btn btn-default btn-back-default btn-back'
  .ibox-content
    .table-responsive
      = datagrid_table @client_enrollment_grid, html: { class: 'table table-bordered table-striped table-hover client_enrollment'}
  .ibox-footer
    = paginate @client_enrollment_grid.assets