- if params[:data] == 'recent'
  - recent_record = enrolled_programs.order(created_at: :desc).first.try(:program_stream).try(:name)
  - if recent_record.present?
    %ul{'data-program_stream': enrolled_programs.count }
      %li= recent_record
- else
  - all_programs = enrolled_programs.map{ |c| c.program_stream.name }.uniq
  - if all_programs.any?
    %ul{'data-program_streams': '' }
      - all_programs.each do |program_name|
        %li= program_name
