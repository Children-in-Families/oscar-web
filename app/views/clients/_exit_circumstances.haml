- if params[:data].presence == 'recent'
  - recent_record = object.exit_ngos.most_recents.first&.exit_circumstance
  - if recent_record.present?
    %ul
      %li= recent_record
- else
  %ul
    - object.exit_ngos.most_recents.each do |exit_ngo|
      - if exit_ngo.exit_circumstance.present?
        %li= exit_ngo.exit_circumstance
