#other-saved-searches.tab-pane{ role: "tabpanel" }
  .table-responsive
    %table.table.table-bordered.table-striped.table-hover
      %thead
        %tr
          %th.name= t('.name')
          %th.description= t('.description')
          %th.owner= t('.owner')
          %th.date= t('.date')
          %th.load-query= t('.load')
      %tbody
        - @other_advanced_searches.each do |advanced_search|
          %tr
            %td.name= advanced_search.name
            %td.description= advanced_search.description
            %td.owner
              = advanced_search.owner
            %td.date
              = date_format(advanced_search.created_at)
            %td.load-query.text-center
              = link_to clients_path(advanced_search.search_params), class: 'btn btn-xs btn-success btn-outline' do
                = fa_icon('clipboard')
