.row
  .col-xs-12
    .ibox
      .ibox-title
        %h5= t('.referral_families')
      .ibox-content
        .row
          .col-xs-12.mini-margin
            %ul.list-group
              - @unsaved_family_referrals.each do |referral|
                %li.list-group-item
                  = link_to "#{referral.name_of_family}", new_family_path(referral_id: referral.id), target: '_blank'
