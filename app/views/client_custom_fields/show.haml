.col-xs-12
  = link_to 'Edit', edit_client_client_custom_field_path(@client_custom_field.client, @client_custom_field), class: 'btn-xs btn btn-success small-btn-margin'
  .ibox
    .ibox-title
      %h5= @client_custom_field.custom_field.form_type
    .ibox-content
      %table.table.table-bordered
        - @client_custom_field.properties_objs.try(:each) do |key, value|
          %tr
            %td.align-justify
              = key.titleize
            %td.align-justify
              = client_properties(value)
