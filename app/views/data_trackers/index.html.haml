.container
  - @versions.each do |version|
    .row
      .col-xs-12.col-sm-3.col-md-4
        = version.object
      .col-xs-12.col-sm-9.col-md-8
        %p= "current #{version.item.agency_ids}"
        %p= "current #{version.reify.agency_ids}" if version.reify
        - version.changeset.each do |k, v|
          %p= "#{k}-#{v[0]} changed to #{v[1]}"
    .row
      .col-xs-12
        %hr