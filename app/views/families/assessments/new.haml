- unless Organization.ratanak?
  .alert.alert-warning.alert-dismissible{ :role => "alert" }
    %button.close{"aria-label" => "Close", "data-dismiss" => "alert", :type => "button"}
      %span{"aria-hidden" => "true"} Ã—
    %ul
      %li= I18n.t('inline_help.assessments.notes.lists.list1').html_safe

.row
  - if current_setting.show_prev_assessment? && @prev_assessment.present?
    .col-md-6#prev-assessment-container
      = render 'unrestricted_form', assessment: @prev_assessment, readonly: true
    .col-md-6
      - if current_setting.disable_required_fields?
        = render 'unrestricted_form', assessment: @assessment, readonly: false
      - else
        = render 'form'
  - else
    .col-md-12
      - if current_setting.disable_required_fields?
        = render 'unrestricted_form', assessment: @assessment, readonly: false
      - else
        = render 'form'

